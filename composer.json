{
    "name": "burxon/api-response-dto",
    "description": "A simple DTO class for consistent API responses in Laravel",
    "keywords": ["laravel", "library", "api-reponse"],
    "type": "library",
    "license": "MIT",
    "version": "v1.1.0",
    "autoload": {
        "psr-4": {
             "Burxon\\ApiResponseDTO\\": "src/"
         },
        "files": [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "saidburxon",
            "email": "19saidburxon96@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^8.0|^9.0|^10.0|^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Burxon\\ApiResponseDTO\\ApiResponseDtoServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true

}
